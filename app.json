{
  "expo": {
    "name": "brewscholar",
    "slug": "brewscholar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BrewScholar.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/BrewScholar.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BrewScholar.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.francispadero.brewscholar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "65418b37-e0a0-4738-a6be-4a78ca823401"
      }
    }
  }
}
